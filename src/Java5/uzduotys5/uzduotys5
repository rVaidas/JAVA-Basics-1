package Java5;

public class uzduotys {
    public static void main(String[] args) {
        int[] masyvas = {1, 2, 3, 4, 5, 6, 7};
        spausdintiMasyva(masyvas, "Musu tekstas");
        int[] masyvas2 = {2,4,0,3,-1};
        spausdintiMasyva(masyvas2, "Antras masyvas"); //butina irasyti bet koki teksta ar bet bet koki skaiciu, nes taip funkcija reikalauja, kad butu sie abui dyd=iai irasyti

    }

    static void spausdintiMasyva(int[] spausdinamasMasyvas, String tekstas) {
        System.out.println(tekstas);
        for(int i = 0; i < spausdinamasMasyvas.length; i++) {
            System.out.println(spausdinamasMasyvas[i]);
        }
    }
}